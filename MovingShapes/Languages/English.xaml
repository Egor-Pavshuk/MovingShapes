<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="circle">Circle</system:String>
    <system:String x:Key="triangle">Triangle</system:String>
    <system:String x:Key="square">Square</system:String>
    <system:String x:Key="stop">Stop</system:String>
    <system:String x:Key="stopAll">Stop all</system:String>
    <system:String x:Key="run">Run</system:String>
    <system:String x:Key="runAll">Run all</system:String>
    <system:String x:Key="language">English</system:String>
    <system:String x:Key="file">File</system:String>
    <system:String x:Key="open">Open</system:String>
    <system:String x:Key="save">Save</system:String>

</ResourceDictionary>